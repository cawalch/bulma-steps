{
  "sourceMaps": false,
  "ignore": ["node_modules/**"],
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": [ ">0.25%", "ie 11"]
      }
    }]
  ],
  "env": {
    "test": {
      "presets": [["env"]]
    }
  },
  "plugins": [
    "transform-object-rest-spread",
    ["transform-runtime", {
      "helpers": false, // defaults to true
      "polyfill": false, // defaults to true
      "regenerator": true, // defaults to true
    }]
  ]
}
